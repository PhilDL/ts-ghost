{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 22",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2022", "ESNext", "DOM", "DOM.Iterable"],
    "module": "commonjs",
    "target": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
